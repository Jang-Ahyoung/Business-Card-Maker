 userId => {\r\n        // history 에서는 push이용해서 우리가 가고 싶은 곳 선택해가면돼\r\n        // 여기선 사용자 정보를 함꼐 전달해줘야해 (로그인했을때 콘솔에 찍히는 유니크 아이디 전달해주면돼)\r\n        // history.push('/maker');\r\n        \r\n        history.push({\r\n            \r\n            pathname:'/maker',\r\n            state:{id:userId}//데이터도 전달\r\n        });\r\n\r\n        // window.location.replace(\"/maker\");\r\n    };\r\n\r\n    useEffect(()=>{\r\n        authService\r\n        .onAuthChange(user => {// (사용자가 바뀌면 무언가 수행하는 함수)콜백을 전달해줘야해\r\n            // 여기에서 사용자가 로그인했다면 user라는 데이터 있을 것이고, 사용자가 로그아웃 했다면 사용자 유저가 null일거야\r\n            user && goToMaker(user.uid);\r\n            // 사용자 있다면 gotomaker로 이동할거야 -> 이렇게 하면 로그인된 정보 있기때문에 바로 maker로 이동하는 것 확인가능<!DOCTYPE html>\r\n            \r\n\r\n\r\n        });\r\n    });\r\n\r\n\r\n    // 로그인 컴포넌트 안에 로그인 할수있는 함수 만들거야\r\n    const onLogin = (event) =>{\r\n        // 이안에는 providerName 전달해줘야해\r\n        authService.login(event.currentTarget.textContent)\r\n        // .then(console.log);\r\n        .then(data => goToMaker(data.user.uid)); // 데이터 받아지면 goToMaker 호출해 데이터 안의 id오브젝트의 uid를 전달해줄거야\r\n    };\r\n    return (\r\n        <section className={styles.container}>\r\n            <Header/>\r\n            <section >\r\n                <div className={styles.login}>\r\n                    <h2 >Login</h2>\r\n                    <p className={styles.desc}>별도의 계정 생성없이 <br/>간편하게 가입하세요 👍</p>\r\n                </div>\r\n                \r\n                <ul className={styles.login_buttom}>\r\n                    {/* 클릭발생하면 저 함수 호출되도록 */}\r\n                    <li className={styles.list}><button className={styles.btn} onClick={onLogin}><img src=\"/images/google.png\" alt=\"google\"/>Google</button></li>\r\n                    <li ><img src=\"/images\" alt=\"\"/><button className={styles.btn} onClick={onLogin}><img src=\"/images/github.png\" alt=\"google\"/>Github</button></li>\r\n                </ul>\r\n            </section>\r\n            {/* <Footer/> */}\r\n        </section>\r\n\r\n    );\r\n\r\n\r\n}\r\nexport default Login;",["70","71"],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\maker\\maker.jsx",["72"],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\service\\auth_service.js",[],["73","74"],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\editor\\editor.jsx",[],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\preview\\preview.jsx",[],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\card\\card.jsx",["75","76"],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\card_editor_form\\card_editor_form.jsx",["77"],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\button\\button.jsx",[],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\image_file_input\\image_file_input.jsx",[],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\index.js",[],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\components\\card_add_form\\card_add_form.jsx",[],"C:\\Users\\kentec3\\Desktop\\waytoGoodCoder\\react-template\\business-card-maker\\src\\service\\card_repository.js",[],{"ruleId":"78","severity":1,"message":"79","line":3,"column":8,"nodeType":"80","messageId":"81","endLine":3,"endColumn":14},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":116,"column":8,"nodeType":"88","endLine":116,"endColumn":16,"suggestions":"89"},{"ruleId":"82","replacedBy":"90"},{"ruleId":"84","replacedBy":"91"},{"ruleId":"78","severity":1,"message":"92","line":7,"column":53,"nodeType":"80","messageId":"81","endLine":7,"endColumn":61},{"ruleId"